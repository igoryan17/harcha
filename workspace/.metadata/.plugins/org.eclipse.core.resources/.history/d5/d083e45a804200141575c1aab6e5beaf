#include <stdio.h>
void swap(int* m, int* n);
void main () {
	int a = 5, b = 10;
	printf ("Input: \n a = %d b = %d\n", a, b);
	swap (&a, &b);

	printf ("Result: \n a = %d b = %d", a, b);
	//return 0;
}
void swap(int* m, int* n)
{
int* c;
c=&m;
m=*b;
n=*c;
}
